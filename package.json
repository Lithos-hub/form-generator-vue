{
	"name": "vue-form-latte",
	"author": "Carlos Segura Garc√≠a",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Lithos-hub/vue-form-latte.git/issues"
	},
	"private": false,
	"version": "0.0.7",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/vue-form-latte.umd.js",
	"module": "./dist/vue-form-latte.es.js",
	"types": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/vue-form-latte.es.js",
			"require": "./dist/vue-form-latte.umd.js"
		},
		"./dist/style.css": "./dist/style.css"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Lithos-hub/vue-form-latte.git"
	},
	"keywords": [
		"form",
		"generator",
		"form-generator",
		"vue3",
		"vue",
		"vuejs",
		"tailwindcss"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build && npm run build:tailwindcss && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
		"build:tailwindcss": "npx tailwindcss -i ./src/lib/style.css -o ./dist/style.css --postcss --minify",
		"preserve": "vite build",
		"serve": "vite preview --port 5050",
		"typecheck": "vue-tsc --noEmit",
		"preview": "vite preview",
		"format": "prettier --write .",
		"lint": "eslint --ext .js,.ts,.vue",
		"lint:fix": "eslint --ext .js,.ts,.vue --fix",
		"test": "vitest",
		"test:w": "vitest --watch",
		"coverage": "vitest run --coverage"
	},
	"dependencies": {
		"vite-plugin-css-injected-by-js": "^3.4.0",
		"vue": "^3.3.11"
	},
	"devDependencies": {
		"@babel/types": "^7.23.9",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/vue": "^8.0.2",
		"@tsconfig/node18": "^18.2.2",
		"@types/jest": "^29.5.12",
		"@types/jsdom": "^21.1.6",
		"@types/node": "^20.11.17",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-vue": "^4.6.2",
		"@vitest/coverage-v8": "^1.2.2",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/test-utils": "^2.4.4",
		"@vue/tsconfig": "^0.5.1",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard-with-typescript": "^43.0.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-vue": "^9.21.1",
		"jsdom": "^24.0.0",
		"npm-run-all2": "^6.1.2",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.8",
		"vite-plugin-dts": "^3.7.2",
		"vitest": "^1.2.2",
		"vue-eslint-parser": "^9.4.2",
		"vue-tsc": "^1.8.25"
	}
}
